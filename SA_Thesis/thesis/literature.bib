@book{Tonti2013,
abstract = {The theories describing seemingly unrelated areas of physics have$\backslash$nsurprising analogies that have aroused the curiosity of scientists$\backslash$nand motivated efforts to identify reasons for their existence. Comparative$\backslash$nstudy of physical theories has revealed the presence of a common$\backslash$ntopological and geometric structure. The Mathematical Structure of$\backslash$nClassical and Relativistic Physics is the first book to analyze this$\backslash$nstructure in depth, thereby exposing the relationship between (a)$\backslash$nglobal physical variables and (b) space and time elements such as$\backslash$npoints, lines, surfaces, instants, and intervals. Combining this$\backslash$nrelationship with the inner and outer orientation of space and time$\backslash$nallows one to construct a classification diagram for variables, equations,$\backslash$nand other theoretical characteristics.$\backslash$n$\backslash$nThe book is divided into three parts. The first introduces the framework$\backslash$nfor the above-mentioned classification, methodically developing a geometric$\backslash$nand topological formulation applicable to all physical laws and properties;$\backslash$nthe second applies this formulation to a detailed study of particle$\backslash$ndynamics, electromagnetism, deformable solids, fluid dynamics, heat$\backslash$nconduction, and gravitation. The third part further analyses the$\backslash$ngeneral structure of the classification diagram for variables and$\backslash$nequations of physical theories.$\backslash$n$\backslash$nSuitable for a diverse audience of physicists, engineers, and mathematicians,$\backslash$nThe Mathematical Structure of Classical and Relativistic Physics$\backslash$noffers a valuable resource for studying the physical world. Written$\backslash$nat a level accessible to graduate and advanced undergraduate students$\backslash$nin mathematical physics, the book can be used as a research monograph$\backslash$nacross various areas of physics, engineering and mathematics, and$\backslash$nas a supplemental text for a broad range of upper-level scientific$\backslash$ncoursework.},
author = {Tonti, Enzo},
booktitle = {Amc},
doi = {10.1007/978-1-4614-7422-7},
file = {:home/lukas/LRZ Sync+Share/Studium/SA/papers/Tonti2013.pdf:pdf},
isbn = {9781461474210},
title = {{The Mathematical Structure of Classical and Relativistic Physics}},
url = {http://link.springer.com/content/pdf/10.1007/978-1-4614-7422-7.pdf},
year = {2013}
