title = {{Parallel strategies for crash and impact simulations}},
url = {http://www.sciencedirect.com/science/article/pii/S0045782599002352$\backslash$nhttp://ac.els-cdn.com.ezproxyd.bham.ac.uk/S0045782599002352/1-s2.0-S0045782599002352-main.pdf?{\_}tid=c20eee56-7a36-11e2-9224-00000aab0f27{\&}acdnat=1361238817{\_}86b62c65b8689183b3dae2ebb8869b25},
