isbn = {9781479928934},
issn = {14708728},
journal = {Science},
keywords = {1,2,5,agonizing pain,conjugate gradient method,convergence analysis,eigen do,eigenvalues,i try,jacobi iterations,preconditioning,thinking with eigenvectors},
number = {CS-94-125},
pages = {64},
pmid = {17348934},
title = {{An Introduction to the Conjugate Gradient Method Without the Agonizing Pain}},
url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.110.418{\&}rep=rep1{\&}type=pdf$\backslash$nhttp://www.cs.cmu.edu/{~}quake-papers/painless-conjugate-gradient.pdf},
